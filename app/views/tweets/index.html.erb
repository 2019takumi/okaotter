<div class="contents">
  <% @tweets.each do |tweet| %>
    <div class="message clearfix" data-id="<%= tweet.user_id %>">
      <%= image_tag 'cat1.png' %>
      <div class="message_box">
        <p class="user_name">
          <%=tweet.user.nickname%>
        </p>
        <p class="text">
          <%= tweet.text %>
        </p>
      </div>
    </div>
  <% end %>
</div>
